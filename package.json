{
  "name": "analytics",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "build": "gridsome build",
    "develop": "gridsome develop",
    "develop-debug": "node --inspect-brk=9229 node_modules/gridsome/bin/gridsome.js develop",
    "explore": "gridsome explore",
    "getdata": "rimraf ./datasrc/COVID-19 && git clone https://github.com/CSSEGISandData/COVID-19 ./datasrc/COVID-19 && yarn run copydata",
    "copydata": "copyfiles -f \"./datasrc/COVID-19/csse_covid_19_data/csse_covid_19_time_series/*\" ./data/csse_covid_19_time_series",
    "test": "jest"
  },
  "dependencies": {
    "@gridsome/plugin-google-analytics": "^0.1.0",
    "@gridsome/source-filesystem": "^0.6.2",
    "@gridsome/transformer-remark": "^0.5.0",
    "@sindresorhus/slugify": "^0.11.0",
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
    "chartjs-plugin-labels": "^1.1.0",
    "d3": "^5.15.1",
    "deck.gl": "^8.1.1",
    "dygraphs": "^2.1.0",
    "gridsome": "^0.7.13",
    "lodash": "^4.17.15",
    "loglevel": "^1.6.7",
    "mapbox-gl": "^1.9.1",
    "moment": "^2.24.0",
    "node-nlp": "^4.3.0",
    "tinycolor2": "^1.4.1",
    "vue-clamp": "^0.3.0",
    "vue-fuse": "^2.2.1",
    "vuetify": "^2.2.20",
    "yarn": "^1.22.15"
  },
  "devDependencies": {
    "bodybuilder": "^2.2.20",
    "copyfiles": "^2.2.0",
    "datalib": "^1.9.2",
    "elasticsearch": "^16.6.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "fs-extra": "^9.0.0",
    "jest": "^25.3.0",
    "node-sass": "^7.0.0",
    "pino": "^5.16.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "prettier": "^1.19.1",
    "ramda": "^0.26.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/test/**/*.test.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
